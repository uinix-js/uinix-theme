{
  "name": "uinix-theme",
  "version": "0.1.0",
  "description": "uinix theme spec and utilties",
  "license": "MIT",
  "keywords": [
    "uinix",
    "theme",
    "spec",
    "design system",
    "ui system"
  ],
  "repository": "uinix-js/uinix-theme",
  "bugs": "https://github.com/uinix-js/uinix-theme/issues",
  "author": "Chris Zhou <chrisrzhou@pm.me> (https://chrisrzhou.io)",
  "type": "module",
  "files": [
    "lib/",
    "index.js",
    "index.d.ts"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "dependencies": {
    "uinix-fp": "^0.1.0"
  },
  "devDependencies": {
    "@types/tape": "^4.13.0",
    "c8": "^7.7.2",
    "rimraf": "^3.0.2",
    "tape": "^5.2.2",
    "type-coverage": "^2.17.5",
    "typescript": "^4.2.4",
    "xo": "^0.39.1"
  },
  "scripts": {
    "clean": "rimraf {lib/**,}*.d.ts",
    "coverage": "npm run type-coverage && npm run test-coverage",
    "lint": "xo --fix",
    "prepack": "npm install && npm run clean && npm run coverage && npm run lint",
    "test-coverage": "c8 --check-coverage --lines 100 --functions 100 --branches 100 --reporter lcov node test/index.js",
    "type-coverage": "tsc && type-coverage",
    "test": "npm run prepack"
  },
  "engines": {
    "node": ">=12"
  },
  "prettier": {
    "bracketSpacing": false,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  },
  "typeCoverage": {
    "atLeast": 100,
    "detail": true,
    "strict": true
  },
  "xo": {
    "esnext": false,
    "prettier": true,
    "rules": {
      "unicorn/no-array-callback-reference": "off",
      "unicorn/no-array-for-each": "off",
      "unicorn/no-array-reduce": "off"
    }
  }
}
